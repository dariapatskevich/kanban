n                <div class=\"card__heading\">\r\n                    <div class=\"card__date-wrap--${theme}\">\r\n                        <p class=\"card__date\">${date}</p>\r\n                        <p class=\"card__time\">${time}</p>\r\n                    </div>\r\n                    <div class=\"card__buttons\">\r\n                        ${isEditable ? '<button class=\"card__edit-btn card__edit-btn--red\"></button>' : ''}                        \r\n                        <button class=\"card__close-btn card__close-btn--${theme}\"></button>\r\n                    </div>\r\n                </div>\r\n                <div class=\"card__body\">\r\n                    <div class=\"card__info\">\r\n                        <div class=\"card__title\">${title}</div>\r\n                        <div class=\"card__description\">${description}</div>\r\n                        <div class=\"card__username\">${username}</div>\r\n                    </div>\r\n                    <button class=\"card__swipe-btn card__swipe-btn--${theme}\"></button>\r\n                </div>\r\n            </div>`\r\n}\r\n\r\nexport const printCards = (data, cardList) => data[cardList.id].map(card => getCardHtml(card)).join('')\r\n\r\nexport const displayUser = (list) => {\r\n  getUsersList().then(userlist => userlist.forEach(user => {\r\n    let option = new Option(user.name, `${user.name}`)\r\n    list.append(option)\r\n  }))\r\n}"},"lineCount":null}},"error":null,"hash":"173750fd2301290142e3b356a9228a02","cacheData":{"env":{}}}